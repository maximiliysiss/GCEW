@model GCEWWeb.Models.ControllerModels.RegisterModel

@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="en">
<head>
    <title>Register</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" type="text/css" href="~/lib/Login_v14/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/lib/Login_v14/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/lib/Login_v14/css/main.css">
</head>
<body>

    <div class="limiter">
        <div class="container-login100">
            <div class="wrap-login100 p-l-62 p-r-62 p-t-45 p-b-45">
                <form class="login100-form validate-form flex-sb flex-w" asp-action="Register" method="post">
                    <label class="login100-form-title p-b-32">
                        Register
                    </label>

                    <label asp-for="Login" class="txt1 p-b-11">
                        Login
                    </label>
                    <div class="wrap-input100 validate-input m-b-36 alert-validate">
                        <input asp-for="Login" class="input100">
                        <span class="focus-input100"></span>
                        <span class="alert-validate-before" asp-validation-for="Login"></span>
                        <div></div>
                    </div>

                    <label asp-for="Nickname" class="txt1 p-b-11">
                        Nickname
                    </label>
                    <div class="wrap-input100 validate-input m-b-36 alert-validate">
                        <input asp-for="Nickname" class="input100">
                        <span class="focus-input100"></span>
                        <span class="alert-validate-before" asp-validation-for="Nickname"></span>
                        <div></div>
                    </div>

                    <label asp-for="Password" class="txt1 p-b-11">
                        Password
                    </label>
                    <div class="wrap-input100 validate-input m-b-36 alert-validate">
                        <input asp-for="Password" class="input100">
                        <span class="focus-input100"></span>
                        <span class="alert-validate-before" asp-validation-for="Password"></span>
                        <div></div>
                    </div>

                    <label asp-for="ConfirmedPassword" class="txt1 p-b-11">
                        Confirmed password
                    </label>
                    <div class="wrap-input100 validate-input m-b-36 alert-validate">
                        <input asp-for="ConfirmedPassword" class="input100">
                        <span class="focus-input100"></span>
                        <span class="alert-validate-before" asp-validation-for="ConfirmedPassword"></span>
                        <div></div>
                    </div>


                    <div class="container-login100-form-btn">
                        <input type="submit" value="Register" class="login100-form-btn" />
                    </div>
                </form>
            </div>

        </div>
    </div>


    <div id="dropDownSelect1"></div>

    <script src="~/lib/jquery/dist/jquery.js"></script>

</body>
</html>